<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:divespots="clr-namespace:Mobile.Features.Divespots"
    xmlns:fillingStations="clr-namespace:Mobile.Features.FillingStations"
    FlyoutBehavior="Disabled"
    Title="Mobile">
    
    <TabBar >
        
        <Tab Title="Divspots">
            <ShellContent
                Title="Dive spots"
                ContentTemplate="{DataTemplate divespots:DivespotsPage}"
                Route="MainPage" />
        </Tab>
        
        <Tab Title="Filling Stations">

            <ShellContent
                Title="Filling Stations"
                ContentTemplate="{DataTemplate fillingStations:FillingStationPage}"
                Route="MainPage" />
            
        </Tab>
        
    </TabBar>


</Shell>
