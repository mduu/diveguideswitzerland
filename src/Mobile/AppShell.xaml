<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:divespots="clr-namespace:Mobile.Features.Divespots"
    FlyoutBehavior="Disabled"
    Title="Mobile">
    
    <TabBar >
        
        <Tab Title="Spots">
            <ShellContent
                Title="My Airplanes"
                ContentTemplate="{DataTemplate divespots:DivespotsPage}"
                Route="MainPage" />
        </Tab>
        
        <Tab Title="Fill">

            <ShellContent
                ContentTemplate="{DataTemplate divespots:DivespotsPage}"
                Route="MainPage" />
            
        </Tab>
        
    </TabBar>


</Shell>
